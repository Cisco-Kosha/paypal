{
  "version": "2.0.0",
  "oauth2_provider_name": "Paypal",
  "fields": [
    {
      "field": "ACCESS_TOKEN",
      "displayName": "OAuth2 Access Token",
      "type": "String",
      "minLength": 50,
      "maxLength": 250,
      "required": true,
      "description": "Paypal OAuth2 access token",
      "example": "A23AAIxEU0XgxEulEa0VKR4Qhx4XmlpB2mHTizaik1000IPjiZax3e7rd4eP5OouNMNFY-laNXIx3q-p-UBt5gFZZemi46mS4A",
      "helpSection": "Paypal OAuth2 access token"
    },
    {
      "field": "REFRESH_TOKEN",
      "displayName": "OAuth2 refresh token",
      "type": "String",
      "minLength": 10,
      "maxLength": 180,
      "required": false,
      "description": "Paypal OAuth2 refresh token",
      "example": "R23.AAITFiOnqFGnOkRY3a_1cT0UpKIdTwkK-952TmZ6f6wmz6dwj3iie6LUuD34UvR1noUUpRv6Dnlxi-u-XwChK44tTMIM3MAGsfYvi0hgtyXToy2lgVxPE6nJWmHziU5sLgMYDa0g4iQzD8XgdCOSqtviUU7I",
      "helpSection": "Paypal OAuth2 refresh token"
    },
    {
      "field": "EXPIRES_AT",
      "displayName": "OAuth2 Expiry Time",
      "type": "String",
      "minLength": 5,
      "maxLength": 30,
      "required": false,
      "description": "Paypal OAuth2 token refresh time",
      "example": "2022-11-18T15:08:55.642141+05:30",
      "helpSection": "Paypal OAuth2 token refresh time"
    },
    {
      "field": "provider",
      "displayName": "OAuth2 Provider Name",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Paypal OAuth2 provider name",
      "example": "paypal",
      "helpSection": "Paypal OAuth2 provider name"
    },
    {
      "field": "email",
      "displayName": "Email fetched from OAuth2 auth flow",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Paypal email fetched from OAuth2 auth flow",
      "example": "bob@email.com",
      "helpSection": "Paypal email fetched from OAuth2 auth flow"
    }
  ]
}